<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="5203283f-fe2c-4fd3-8289-2a773b56fd03" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Step.2 - NPM Build</name>
  <description />
  <settings ref="Step2npmBuild">
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_5" sourceBuildTypeId="AngularApp_Build" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="+:target_directory/node_modules.tar.gz =&gt; %teamcity.build.workingDir%" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="AngularApp_Build">
        <options>
          <option name="run-build-on-the-same-agent" value="true" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

