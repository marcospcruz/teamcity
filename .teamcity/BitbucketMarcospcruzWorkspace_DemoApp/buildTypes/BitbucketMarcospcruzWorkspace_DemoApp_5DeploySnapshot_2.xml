<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="aacb66df-98ba-4b5c-bdf0-1c253b873f77" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>5.Deploy Snapshot</name>
  <description />
  <settings ref="BitbucketMarcospcruzWorkspace_DemoApp_5DeploySnapshotTemplate">
    <parameters />
    <build-runners>
      <runner id="RUNNER_25" name="mvn deploy snapshot" type="Maven2">
        <conditions>
          <contains name="teamcity.build.branch" value="feature" />
          <does-not-contain name="teamcity.build.branch" value="master" />
        </conditions>
        <parameters>
          <param name="goals" value="deploy" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="runnerArgs" value="-DskipTests" />
          <param name="target.jdk.home" value="%env.JDK_17_0_ARM64%" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
          <param name="teamcity.coverage.emma.instr.parameters" value="-ix -*Test*" />
          <param name="teamcity.coverage.idea.includePatterns" value="*" />
          <param name="teamcity.coverage.jacoco.patterns" value="+:*" />
          <param name="teamcity.step.mode" value="default" />
          <param name="teamcity.tool.jacoco" value="%teamcity.tool.jacoco.DEFAULT%" />
          <param name="userSettingsSelection" value="settings.xml" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="BitbucketMarcospcruzWorkspace_DemoApp_4Vulnerabilities_2">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

