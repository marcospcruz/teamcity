<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="455d3da2-2ba8-4519-ba27-718858ed590b" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Integration Test</name>
  <description />
  <settings>
    <parameters>
      <param name="add-all-branches"><![CDATA[%exclude-master%%
%add-feature-and-bugfix%
%add-release-hotfix%
%add-develop%]]></param>
    </parameters>
    <build-runners>
      <runner id="RUNNER_27" name="Verify" type="Maven2">
        <parameters>
          <param name="goals" value="verify" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="teamcity.coverage.emma.include.source" value="true" />
