          <param name="script.content"><![CDATA[export WEB_IMAGE=apuracao-pleito/${WEB_DOCKERFILE}
export WEB_JAR="target/${WEB_DOCKERFILE}-0.0.2-SNAPSHOT.jar"

chmod 755 docker_build_image.sh
./docker_build_image.sh]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="ApuracaoPleitoStack_HttpsGithubComMarcospcruzApuracaoPleitoStackRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_3" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="perfmon" type="perfmon">
        <parameters>
          <param name="teamcity.perfmon.feature.enabled" value="true" />
